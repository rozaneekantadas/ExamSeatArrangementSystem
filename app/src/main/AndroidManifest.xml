<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.suptodas.diu.examseatarrangementsystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ShowExamTimeTable"
            android:theme="@style/CustomAppTheme"/>

        <activity
            android:name=".ShowExamData"
            android:theme="@style/CustomAppTheme" />

        <activity android:name=".PrintActivity" />
        <activity
            android:name=".HomeActivity"
            android:theme="@style/CustomAppTheme" />

        <activity android:name=".AddCourseInfo"
            android:theme="@style/CustomAppTheme" />

        <activity
            android:name=".CourseTimeSelection"
            android:theme="@style/CustomAppTheme" />

        <activity
            android:name=".TimeTableSelection"
            android:theme="@style/CustomAppTheme" />

        <activity
            android:name=".MainActivity"
            android:theme="@style/CustomAppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>